file vac_ctrl_tpg300_sensor.template
{
	pattern { SENSOR,  SLOT,  SOURCE }

		{ A1,      A,     A }
		{ A2,      A,     B }
		{ B1,      B,     C }
		{ B2,      B,     D }
}

